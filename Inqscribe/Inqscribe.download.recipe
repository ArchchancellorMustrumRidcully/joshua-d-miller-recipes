<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Description</key>
	<string>Downloads the latest verison of Inqscribe</string>
	<key>Identifier</key>
	<string>com.github.joshua-d-miller.download.inqscribe</string>
	<key>Input</key>
	<dict>
        <key>Processor</key>
        <string>URLTextSearcher</string>
        <key>Arguments</key>
        <dict>
            <key>url</key>
            <string>https://www.inqscribe.com/download.html</string>
            <key>re_pattern</key>
            <string>(?P&lt;URL&gt;http:\/\/files\.inqscribe\.com\/InqScribe_.*?\.dmg).*&lt;br&gt;
.*&lt;br&gt;
.*current version</string>
        </dict>
    </dict>
	<key>MinimumVersion</key>
	<string>0.2.0</string>
	<key>Process</key>
	<array>
        <dict>
            <key>Processor</key>
            <string>URLDownloader</string>
            <key>Arguments</key>
            <dict>
                <key>url</key>
                <string>%URL%</string>
                <key>filename</key>
                <string>%NAME%.dmg</string>
            </dict>
        </dict>
		<dict>
			<key>Processor</key>
			<string>EndOfCheckPhase</string>
		</dict>
	</array>
</dict>
</plist>
